    <form action="/supermercado/store" method="post">
        <div class="form-group">
            <label for="campo-cnpj">Cnpj</label>
            <input class="form-control" type="text" id="campo-cnpj" name="supermercado.Cnpj" required="required" />
        </div>
        <div class="form-group">
            <label for="campo-nome">Nome</label>
            <input class="form-control" type="text" id="campo-nome" name="supermercado.Nome" required="required" />
        </div>
        <div class="form-group">
            <label for="campo-faturamento">Faturamento</label>
            <input class="form-control" type="number" id="campo-faturamento" name="supermercado.Faturamento" required="required" />
        </div>
        <div class="float-right mb-4">
            <button class="btn btn-success" type="submit" value="salvar"><i class="far fa-save"></i> Salvar</button>
        </div>
    </form>
